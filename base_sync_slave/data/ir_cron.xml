<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="base_sync_import_first_cron" model="ir.cron">
        <field name="name">Base Sync Import First Cron</field>
        <field name="model_id" ref="base_sync_slave.model_base_sync"/>
        <field name="state">code</field>
        <field name="code">model._action_cron()</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record id="base_sync_import_second_cron" model="ir.cron">
        <field name="name">Base Sync Import Second Cron</field>
        <field name="model_id" ref="base_sync_slave.model_base_sync"/>
        <field name="state">code</field>
        <field name="code">model._action_cron_2()</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">3</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>
</odoo>
