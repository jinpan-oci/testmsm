<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="IframeCustomWidget" owl="1">
        <iframe
            t-if="url"
            height="100%"
            width="100%"
            frameBorder="0"
            t-att-src="url"
            class='d-block'
        ></iframe>
        <div t-else="">No Url</div>
    </t>

</templates>
